<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/cardImageView1"
        android:layout_width="73dp"
        android:layout_height="100dp"
        android:layout_marginTop="72dp"
        android:padding="5dp"
        app:layout_constraintStart_toEndOf="@+id/cardImageView0"
        app:layout_constraintTop_toBottomOf="@+id/cardImageView4"
        app:srcCompat="@drawable/unused" />

    <ImageView
        android:id="@+id/cardImageView0"
        android:layout_width="73dp"
        android:layout_height="100dp"
        android:layout_marginTop="56dp"
        android:padding="5dp"
        app:layout_constraintTop_toBottomOf="@+id/cardImageView3"
        app:srcCompat="@drawable/unused"
        tools:layout_editor_absoluteX="81dp" />

    <ImageView
        android:id="@+id/cardImageView6"
        android:layout_width="73dp"
        android:layout_height="100dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:padding="5dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/cardImageView5"
        app:srcCompat="@drawable/unused"
        tools:layout_editor_absoluteY="16dp" />

    <ImageView
        android:id="@+id/cardImageView5"
        android:layout_width="73dp"
        android:layout_height="100dp"
        android:padding="5dp"
        app:layout_constraintStart_toEndOf="@+id/cardImageView4"
        app:srcCompat="@drawable/unused"
        tools:layout_editor_absoluteY="0dp" />

    <ImageView
        android:id="@+id/cardImageView4"
        android:layout_width="73dp"
        android:layout_height="100dp"
        android:padding="5dp"
        app:layout_constraintStart_toEndOf="@+id/cardImageView3"
        app:srcCompat="@drawable/unused"
        tools:layout_editor_absoluteY="0dp" />

    <ImageView
        android:id="@+id/cardImageView3"
        android:layout_width="73dp"
        android:layout_height="100dp"
        android:padding="5dp"
        app:layout_constraintStart_toEndOf="@+id/cardImageView2"
        app:srcCompat="@drawable/unused"
        tools:layout_editor_absoluteY="256dp" />

    <ImageView
        android:id="@+id/cardImageView2"
        android:layout_width="73dp"
        android:layout_height="100dp"
        android:layout_marginTop="16dp"
        android:padding="5dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/unused" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/addCardButton"
        android:layout_width="45dp"
        android:layout_height="42dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="36dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="32dp"
        android:clickable="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/cardImageView1"
        app:layout_constraintTop_toBottomOf="@+id/cardImageView5"
        app:srcCompat="@android:drawable/ic_input_add" />

</android.support.constraint.ConstraintLayout>